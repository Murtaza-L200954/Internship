   city VARCHAR(20),\n    state_province VARCHAR(20),\n    postal_code VARCHAR(10),\n    country_id VARCHAR(20)\n)\nINSERT INTO Locations\nVALUES\n('75','194-A, Model Town','Lahore','Punjab','0967','Pakistan'),\n('76','164-B, Model Town','Lahore','Punjab','1067','Pakistan'),\n('80','8-A, Muslim Town','Lahore','Punjab','0907','Pakistan'),\n('85','19-D, ABC Town','Islamabad','Punjab','0537','Pakistan'),\n('90','11-C, XYZ Town','Peshawar','KPK','0856','Pakistan'),\n('95','14-E DEF Town','Karachi','Sindh','0152','Pakistan')\nSELECT * \nFROM Employees \nWHERE department_id = 90\nSELECT first_name,last_name,salary\nFROM Employees\nORDER BY last_name ASC\nSELECT * \nFROM Employees\nORDER BY salary DESC\nLIMIT 5\nSELECT e.employee_id,e.first_name,j.job_title\nFROM Employees e\nJOIN Jobs j ON e.job_id = j.job_id\nWHERE j.job_title LIKE '%IT%'\nSELECT \n    e.first_name,\n    e.last_name,\n    j.job_title\nFROM employees e\nJOIN jobs j ON e.job_id = j.job_id\nSELECT \n    department_id,\n    AVG(salary) AS average_salary\nFROM employees\nGROUP BY department_id\nSELECT j.job_title, COUNT(e.employee_id) AS employee_count\nFROM Employees e\nJOIN Jobs j ON e.job_id = j.job_id\nGroup BY j.job_title\nSELECT d.department_name,\nCOUNT(e.employee_id) AS employee_count\nFROM Departments d\nJOIN Employees e ON d.department_id = e.department_id\nGROUP BY d.department_name\nHAVING COUNT(e.employee_id) > 5",
